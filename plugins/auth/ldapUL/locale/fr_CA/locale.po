msgid "plugins.auth.ldap.ul.displayName"
msgstr "Protocole LDAP UL"

msgid "plugins.auth.ldap.ul.description"
msgstr "Ce plugiciel permet l'authentification et la synchronisation de comptes d'utilisateurs au moyen d'une source de données LDAP UL."

msgid "plugins.auth.ldap.ul.user.no.valid.email"
msgstr "Le compte LDAP ne contient pas d'adresse courriel valide. Veuillez contacter l'administrateur du site pour obtenir de l'aide."

msgid "plugins.auth.ldap.ul.user.already.binded"
msgstr "Un compte LDAP est déjà lié au compte OJS possèdant votre adresse courriel. Veuillez contacter l'administrateur du site pour obtenir de l'aide."

msgid "plugins.auth.ldap.ul.user.username.already.exists"
msgstr "Un compte OJS possèdant le même nom d'utilisateur existe déjà. Veuillez contacter l'administrateur du site pour obtenir de l'aide."
