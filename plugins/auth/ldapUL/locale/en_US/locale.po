msgid "plugins.auth.ldap.ul.displayName"
msgstr "LDAP UL Protocol"

msgid "plugins.auth.ldap.ul.description"
msgstr "This plugin allows for authentication and synchronization of user accounts against an UL LDAP data source."

msgid "plugins.auth.ldap.ul.user.no.valid.email"
msgstr "The LDAP account doesn't contain a valid email. Please contact the site administrator for help."

msgid "plugins.auth.ldap.ul.user.already.binded"
msgstr "An LDAP account is already linked to the OJS account having your email address. Please contact the site administrator for help."

msgid "plugins.auth.ldap.ul.user.username.already.exists"
msgstr "An OJS account having the same username already exists. Please contact the site administrator for help."
