msgid "plugins.generic.fileFormatFilter.name"
msgstr "File Format Filter Plugin"

msgid "plugins.generic.fileFormatFilter.description"
msgstr "This plugin allows, for each genre, the creation of a list of accepted or refused file formats."

msgid "plugins.generic.fileFormatFilter.settings.title"
msgstr "List of all file formats to include or exclude."

msgid "plugins.generic.fileFormatFilter.settings.include"
msgstr "Include"

msgid "plugins.generic.fileFormatFilter.settings.exclude"
msgstr "Exclude"

msgid "plugins.generic.fileFormatFilter.settings.hint"
msgstr "The formats must be separated by the character \",\"."

msgid "plugins.generic.fileFormatFilter.settings.placeholder"
msgstr "Example: pdf, doc, docx"

msgid "plugins.generic.fileFormatFilter.uploadFailed.format.exclusion"
msgstr "The file could not be uploaded because the format is not supported for this document type. Here is the list of prohibited formats: {$formats}."

msgid "plugins.generic.fileFormatFilter.uploadFailed.format.inclusion"
msgstr "The file could not be uploaded because the format is not supported for this document type. Here is the list of authorized formats: {$formats}."

msgid "plugins.generic.fileFormatFilter.uploadFailed.format.empty"
msgstr "The file could not be uploaded because it does not have any format."
